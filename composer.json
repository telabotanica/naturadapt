{
    "name": "telabotanica/pollinisateurs",
    "description": "Nos Pollinisateurs community website",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.2",
        "doctrine/annotations": "^1.14.3",
        "doctrine/doctrine-bundle": "^2.7.2",
        "doctrine/doctrine-fixtures-bundle": "^3.4.2",
        "doctrine/doctrine-migrations-bundle": "^3.2.2",
        "doctrine/orm": "^2.14.1",
        "fakerphp/faker": "^1.21.0",
        "gaufrette/local-adapter": "^0.4.0",
        "knplabs/gaufrette": "^0.9.0",
        "knplabs/knp-gaufrette-bundle": "^0.7.1",
        "knplabs/knp-time-bundle": "^1.19",
        "liip/imagine-bundle": "^2.10",
        "phpdocumentor/reflection-docblock": "^5.3",
        "ramsey/uuid": "^3.9.7",
        "sensio/framework-extra-bundle": "^5.6.1",
        "symfony/apache-pack": "^1.0.1",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.19.4",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/process": "4.4.*",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/swiftmailer-bundle": "^3.5.4",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/var-dumper": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.16.1",
        "symfony/yaml": "4.4.*",
        "teamtnt/tntsearch": "^2.9",
        "twig/extensions": "^1.5.4",
        "wildbit/postmark-php": "^2.11",
        "wildbit/swiftmailer-postmark": "^3.4",
        "willdurand/email-reply-parser": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/debug-bundle": "4.4.*",
        "symfony/maker-bundle": "^1.39.1",
        "symfony/phpunit-bridge": "6.2.*",
        "symfony/stopwatch": "4.4.*",
        "symfony/web-profiler-bundle": "4.4.*",
        "symfony/web-server-bundle": "4.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "8.1.15"
        },
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
