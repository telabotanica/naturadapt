<div id="{{id_liens}}_linkList"
	class="liens"
	data-prototype="{{ form_widget(liens.vars.prototype)|e('html_attr') }}"
	data-widget-tags="{{ '<li></li>'|e }}"
	data-widget-counter="{{ liens|length }}">
	{% for lien in liens %}
		<div id="{{id_liens}}_{{ loop.index0 }}" class="columns__36-36-28">
			<div class="form-row-aligned ">
				{{ form_label(lien.nom, 'forms.administration.menus.label') }}
				{{ form_widget(lien.nom) }}
			</div>
			<div  class="form-row-aligned ">
				{{ form_label(lien.lien, 'forms.administration.menus.link') }}
				{{ form_widget(lien.lien) }}
			</div >
			<div  class="form-row-aligned ">
				<label>{{ 'forms.administration.menus.order'|trans  }}</label>
				<button type="button" class="order_link_change order_link_change_up"></button>
				<button type="button" class="order_link_change order_link_change_down"></button>
			</div >
		</div>
	{% endfor %}
</div>
